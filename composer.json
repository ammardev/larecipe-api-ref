{
    "name": "ammardev/larecipe-api-ref",
    "description": "A LaRecipe asset.",
    "keywords": [
        "laravel",
        "larecipe"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Ammardev\\LarecipeApiRef\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ammardev\\LarecipeApiRef\\AssetServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
